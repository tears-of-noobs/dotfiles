Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
URxvt.depth:                24
URxvt.transparent:          false
URxvt.fading:               0
URxvt.urgentOnBell:         true
URxvt.loginShell:           true
URxvt.saveLines:            5000
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt.termName: rxvt-unicode-256color
URxvt.allow_bold:           false
!URxvt*font:                 xft:Monaco:style=Regular:pixelsize=12
!URxvt*boldFont:             xft:Monaco:style=Bold:pixelsize=12
URxvt*font:                 xft:Hack-Regular:pixelsize=11
URxvt*boldFont:             xft:Hack-Bold:pixelsize=11
URxvt*letterSpace:          -1
URxvt.scrollBar:            false

! Perl extensions
URxvt.perl-ext-common:      url-select,keyboard-select
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.matcher.button:       1
URxvt.urlLauncher:          chromium

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true
URxvt.keysym.Mod4-n: string:\033\033[24~
URxvt.keysym.Mod4-slash: string:\033\033[24~?

#include ".Xdefaults/rxvt.theme"

!*fading:                  40


